{% extends "layout.njk" %} 
{% block head %}
<title>Homepage | {{ siteName }}</title>
{% endblock %} 
{% block content %}
<div id="header-gradient"></div>
<div id="main-content" tabindex="-1">
      <!-- The main page content -->
      
      <!-- Homepage Opener Options -->
      <div class="container-fluid">
            {% include "partials/homepage-header-options/homepage-opener-full-width-image.njk" %}
            {% include "partials/homepage-header-options/homepage-opener-carousel-full-width-image.njk" %}
            {% include "partials/homepage-header-options/homepage-opener-carousel-small-image.njk" %}
            {% include "partials/homepage-header-options/homepage-opener-abstract-and-image.njk" %}
      </div>
      

      <!-- Basic Page Content -->
      <div class="container">
            <div class="row">
                  <div class="col-12 col-lg-8 m-auto">
                        {% include "partials/basic-page-content.njk" %}
                        {% include "partials/basic-quote.njk" %}
                  </div>
            </div>
      </div>
      
</div>
{% endblock %}